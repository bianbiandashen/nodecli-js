  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-app-server --sticky --ignore-stderr",
    "stop": "egg-scripts stop --title=egg-server-app-server",
    "reboot": "egg-scripts reboot --daemon --title=egg-server-app-server --sticky --ignore-stderr",
    "kill": "egg-scripts kill --title=egg-server-app-server",
    "dev": "egg-bin dev --sticky",
    "debug": "egg-bin debug --sticky --stdout=./logs/stdouterr/master-stdout.log --stderr=./logs/stdouterr/master-stderr.log",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint ./app/controller",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "db:init": "npx sequelize migration:generate --name",
    "db:up": "npx sequelize db:migrate",
    "db:down": "npx sequelize db:migrate:undo",
    "db:down-all": "npx sequelize db:migrate:undo:all",
    "cli": "egg-ebg-model-tools",
    "postinstall": "node ./puppeteer-install.js",
    "pwd": "npx ./resolvePWD.js"
  },
  "ci": {
    "version": "8"
  },
